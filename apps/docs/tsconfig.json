{
  "extends": "@ezcmd/ts-config/docs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/.source": ["./.source/index.ts"],
      "@/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
